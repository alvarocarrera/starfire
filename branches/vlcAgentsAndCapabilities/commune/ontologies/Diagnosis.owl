<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns="http://www.owl-ontologies.com/Diagnosis.owl#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
  xml:base="http://www.owl-ontologies.com/Diagnosis.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://swrl.stanford.edu/ontologies/3.3/swrla.owl"/>
    <owl:imports rdf:resource="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Diagnosis">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents the concept of diagnosis, i.e. the procedure to 
      find the root cause failure.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Device">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="System"/>
          <owl:Class>
            <owl:complementOf>
              <owl:Class rdf:ID="Subsystem"/>
            </owl:complementOf>
          </owl:Class>
          <owl:Class rdf:ID="HardwareSystem"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#HardwareSystem">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#System"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="SoftwareSystem"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Subsystem">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#System"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#SoftwareSystem">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#System"/>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#HardwareSystem"/>
  </owl:Class>
  <owl:Class rdf:ID="Failure">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents the concept of failure.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="UnsatisfiedCondition">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents conditions that are not satisfied.
    &lt;/p&gt;</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="SatisfiedCondition"/>
    </owl:disjointWith>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="Condition"/>
          <owl:Class>
            <owl:complementOf>
              <owl:Class rdf:about="#SatisfiedCondition"/>
            </owl:complementOf>
          </owl:Class>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Human">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents human actors that can perform actions manually.
    &lt;/p&gt;</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Actor"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Agent"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="UnsuccessfulyPerformedAction">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents an action that has been unsuccessfuly performed by 
      an actor, i.e. not all its postconditions are satisfied.
    &lt;/p&gt;</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="SuccessfulyPerformedAction"/>
    </owl:disjointWith>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="PerformedAction"/>
          <owl:Class>
            <owl:complementOf>
              <owl:Class rdf:about="#SuccessfulyPerformedAction"/>
            </owl:complementOf>
          </owl:Class>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#Actor">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents any actor that can perform actions.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Network">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents a network of connected devices.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Symptom">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents the symtom that starts a diagnosis.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="MonitoringAction">
    <owl:disjointWith>
      <owl:Class rdf:ID="TestAction"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="HealingAction"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="EnvironmentAction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#HealingAction">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents an action that can repair a failure.
    &lt;/p&gt;</rdfs:comment>
    <owl:disjointWith rdf:resource="#MonitoringAction"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TestAction"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#EnvironmentAction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#EnvironmentAction">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Action"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents actions that can be executed in the environment, 
      for example: external systems.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="RequiredActorCondition">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This type of condition represents that an action needs an actor able to 
      perform it.
    &lt;/p&gt;</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Condition"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#SatisfiedCondition">
    <owl:disjointWith rdf:resource="#UnsatisfiedCondition"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Condition"/>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="satisfied"/>
            </owl:onProperty>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >true</owl:hasValue>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents conditions that are satisfied.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#SuccessfulyPerformedAction">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#PerformedAction"/>
          <owl:Restriction>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >true</owl:hasValue>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="successfullyPerformed"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents an action that has been successfuly performed by 
      an actor, i.e. all its postconditions are satisfied.
    &lt;/p&gt;</rdfs:comment>
    <owl:disjointWith rdf:resource="#UnsuccessfulyPerformedAction"/>
  </owl:Class>
  <owl:Class rdf:about="#System">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This concept represents systems that exist in the scenario.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Condition">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#satisfied"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents conditions of actions.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#PerformedAction">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Action"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="performed"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents an action that has been performed by an actor.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Connection">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents the connection between devices.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#TestAction">
    <owl:disjointWith rdf:resource="#MonitoringAction"/>
    <owl:disjointWith rdf:resource="#HealingAction"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents an action that can test a device, a systems, etc.
    &lt;/p&gt;</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#EnvironmentAction"/>
  </owl:Class>
  <owl:Class rdf:about="#Agent">
    <rdfs:subClassOf rdf:resource="#Actor"/>
    <owl:disjointWith rdf:resource="#Human"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents software actors that can automate actions.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="AvailableAction">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Action"/>
          <owl:Restriction>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >true</owl:hasValue>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="available"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class is used to classify actions which all its preconditions are 
      satisfied.
    &lt;/p&gt;</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="UnavailableAction"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Hypothesis">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents the hypothesis of a failure. It has a associated 
      confidence to represent how many certainty is about the occurrence of a 
      failure.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Variable">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents data of the scenario. For example: variableType: 
      clientIP variableValue: 192.168.0.100
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#UnavailableAction">
    <owl:disjointWith rdf:resource="#AvailableAction"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Action"/>
          <owl:Class>
            <owl:complementOf rdf:resource="#AvailableAction"/>
          </owl:Class>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class is used to classify actions which not all its preconditions 
      are satisfied.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Action">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#available"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasPrecondition"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:valuesFrom rdf:resource="#RequiredActorCondition"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;
      This class represents an action that can be performed by an actor at any 
      time.
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Observation"/>
  <owl:ObjectProperty rdf:ID="startsDiagnosis">
    <rdfs:domain rdf:resource="#Symptom"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="startedBySymptom"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#Diagnosis"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="performedTestAction">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="performedAction"/>
    </rdfs:subPropertyOf>
    <rdfs:range>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#TestAction"/>
          <owl:Class rdf:about="#PerformedAction"/>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="resultSymptom">
    <rdfs:domain rdf:resource="#MonitoringAction"/>
    <rdfs:range rdf:resource="#Symptom"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isPartOf">
    <rdfs:range rdf:resource="#System"/>
    <rdfs:domain rdf:resource="#Subsystem"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isComposedBy"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPerformed">
    <rdfs:domain rdf:resource="#Actor"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasBeenPerformedBy"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#PerformedAction"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasAgentResiding">
    <rdfs:domain rdf:resource="#System"/>
    <owl:inverseOf>
      <owl:FunctionalProperty rdf:ID="isExecutingIn"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#Agent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isPartOfNetwork">
    <rdfs:range rdf:resource="#Network"/>
    <rdfs:domain rdf:resource="#Connection"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasConnection"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasPrecondition">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="hasCondition"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="connectsSystem">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isConnectedBy"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#Connection"/>
    <rdfs:range rdf:resource="#Device"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="canBeRepairedWith">
    <rdfs:range rdf:resource="#HealingAction"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="repairsFailure"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#Failure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="belongsToNetwork">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasDevice"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#Network"/>
    <rdfs:domain rdf:resource="#Device"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="performedMonitoringAction">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#performedAction"/>
    </rdfs:subPropertyOf>
    <rdfs:range>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#MonitoringAction"/>
          <owl:Class rdf:about="#PerformedAction"/>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#startedBySymptom">
    <rdfs:domain rdf:resource="#Diagnosis"/>
    <owl:inverseOf rdf:resource="#startsDiagnosis"/>
    <rdfs:range rdf:resource="#Symptom"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="performedHealingAction">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#performedAction"/>
    </rdfs:subPropertyOf>
    <rdfs:range>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#HealingAction"/>
          <owl:Class rdf:about="#PerformedAction"/>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#repairsFailure">
    <owl:inverseOf rdf:resource="#canBeRepairedWith"/>
    <rdfs:domain rdf:resource="#HealingAction"/>
    <rdfs:range rdf:resource="#Failure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasConnection">
    <rdfs:range rdf:resource="#Connection"/>
    <owl:inverseOf rdf:resource="#isPartOfNetwork"/>
    <rdfs:domain rdf:resource="#Network"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasBeenPerformedBy">
    <owl:inverseOf rdf:resource="#hasPerformed"/>
    <rdfs:range rdf:resource="#Actor"/>
    <rdfs:domain rdf:resource="#PerformedAction"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="canBePerformedBy">
    <rdfs:range rdf:resource="#Actor"/>
    <rdfs:domain rdf:resource="#Action"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="canPerform"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#canPerform">
    <rdfs:domain rdf:resource="#Actor"/>
    <owl:inverseOf rdf:resource="#canBePerformedBy"/>
    <rdfs:range rdf:resource="#Action"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#performedAction">
    <rdfs:domain rdf:resource="#Diagnosis"/>
    <rdfs:range rdf:resource="#PerformedAction"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasHypothesis">
    <rdfs:range rdf:resource="#Hypothesis"/>
    <rdfs:domain rdf:resource="#Diagnosis"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasCondition">
    <rdfs:range rdf:resource="#Condition"/>
    <rdfs:domain rdf:resource="#Action"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasDevice">
    <owl:inverseOf rdf:resource="#belongsToNetwork"/>
    <rdfs:domain rdf:resource="#Network"/>
    <rdfs:range rdf:resource="#Device"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isConnectedBy">
    <rdfs:domain rdf:resource="#Device"/>
    <owl:inverseOf rdf:resource="#connectsSystem"/>
    <rdfs:range rdf:resource="#Connection"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isComposedBy">
    <rdfs:range rdf:resource="#Subsystem"/>
    <rdfs:domain rdf:resource="#System"/>
    <owl:inverseOf rdf:resource="#isPartOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPostcondition">
    <rdfs:subPropertyOf rdf:resource="#hasCondition"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="possibleResults">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="hasLocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Failure"/>
          <owl:Class rdf:about="#Symptom"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="hasFailure"/>
    </owl:inverseOf>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="id">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Actor"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="whenHasFinished">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="expectedBenefit">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#TestAction"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#successfullyPerformed">
    <rdfs:domain rdf:resource="#PerformedAction"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#satisfied">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#Condition"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="residesIn">
    <rdfs:domain rdf:resource="#Variable"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="hasData"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#System"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hypothesisConfidence">
    <rdfs:domain rdf:resource="#Hypothesis"/>
